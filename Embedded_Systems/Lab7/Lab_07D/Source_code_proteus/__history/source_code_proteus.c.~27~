  // SETUP_TIMER_0(T0_INTERNAL | T0_DIV_64 );
   
   //enable_interrupts(INT_TIMER0);   
   
   //enable_interrupts(GLOBAL);
   
   //set_timer0(56161);
   
   counter = 0;
}

void main(void){
   while(TRUE){}
}

